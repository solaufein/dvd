<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

    <definition name="defaultTemplate" template="/WEB-INF/templates/template.jsp">
        <put-attribute name="header" value="/WEB-INF/templates/header.jsp" />
        <put-attribute name="login_info" value="/WEB-INF/templates/loginInfo.jsp" />
        <put-attribute name="navigation" value="/WEB-INF/templates/navigation.jsp" />
        <put-attribute name="langs" value="/WEB-INF/templates/langs.jsp" />
        <put-attribute name="footer" value="/WEB-INF/templates/footer.jsp" />
    </definition>

    <definition name="extended" extends="defaultTemplate">
        <!-- Overridden here -->
        <put-attribute name="head" value="/WEB-INF/jsp/xxx.jsp" />

    </definition>

</tiles-definitions>